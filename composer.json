{
  "name": "woolf/carter",
  "description": "Shopify app jumpstart for Laravel",
  "license": "MIT",
  "homepage": "https://github.com/nickywoolf/carter",
  "autoload": {
    "psr-4": {
      "Woolf\\Carter\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "woolf/shophpify": "dev-master",
    "illuminate/contracts": "^5.2",
    "illuminate/support": "^5.2"
  },
  "repositories": [
      { "type": "vcs", "url": "https://github.com/81552433qqcom/shophpify" }
  ],
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "^0.9.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Woolf\\Carter\\Tests\\": "tests/"
    }
  }
}
